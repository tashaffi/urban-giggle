[ req ]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn

[ dn ]
C = US
ST = California
L = Los Angeles
O = My Company
OU = My Unit
emailAddress = my-email@example.com
CN = www.example.com

[ ext ]
basicConstraints = CA:FALSE
keyUsage = digitalSignature
extendedKeyUsage = codeSigning